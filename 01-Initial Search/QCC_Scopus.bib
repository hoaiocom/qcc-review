@ARTICLE{Jayamala2024133,
	author = {Jayamala, R. and Valarmathi, A.},
	title = {FSRmSTS—An Optimize Task Scheduling with a Hybrid Approach: Integrating FCFS, SJF, and RR with Median Standard Time Slice},
	year = {2024},
	journal = {Studies in Big Data},
	volume = {151},
