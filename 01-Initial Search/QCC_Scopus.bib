@CONFERENCE{Belmahdi2022131,
	author = {Belmahdi, Raouf and Mechta, Djamila and Harous, Saad and Bentaleb, Abdelhak},
	title = {SQGA: Quantum Genetic Algorithm-based Workflow Scheduling in Fog-Cloud Computing},
