	affiliations = {Universidad de Extremadura, Quercus Software Engineering Group, Cáceres, Spain; Computing and Advanced Technologies Foundation of Extremadura, Cáceres, Spain; Itis Software, Universidad de Málaga, Málaga, Spain},
