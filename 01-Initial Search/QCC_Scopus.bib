	doi = {10.1109/IWCMC55113.2022.9825324},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85135292018&doi=10.1109%2fIWCMC55113.2022.9825324&partnerID=40&md5=5bd85323c5889d3677185809ae1e4ed6},
