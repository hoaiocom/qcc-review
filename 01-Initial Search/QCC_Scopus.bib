	title = {Relevance of Near-Term Quantum Computing in the Cloud: A Humanities Perspective},
	year = {2021},
	journal = {Communications in Computer and Information Science},
	volume = {1399 CCIS},
	pages = {25 – 58},
	doi = {10.1007/978-3-030-72369-9_2},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85102669410&doi=10.1007%2f978-3-030-72369-9_2&partnerID=40&md5=568e622d7b67c78b5a778bae44e16c8e},
	affiliations = {University of Stuttgart, Universitätsstraße 38, Stuttgart, 70569, Germany; StoneOne AG, Keithstraße 6, Berlin, 10787, Germany},
	abstract = {As quantum computers are becoming real, they have the inherent potential to significantly impact many application domains. In this paper we outline the fundamentals about programming quantum computers and show that quantum programs are typically hybrid consisting of a mixture of classical parts and quantum parts. With the advent of quantum computers in the cloud, the cloud is a fine environment for performing quantum programs. The tool chain available for creating and running such programs is sketched. As an exemplary problem we discuss efforts to implement quantum programs that are hardware independent. A use case from quantum humanities is discussed, hinting which applications in this domain can already be used in the field of (quantum) machine learning. Finally, a collaborative platform for solving problems with quantum computers – that is currently under construction – is presented. © 2021, Springer Nature Switzerland AG.},
	author_keywords = {Cloud computing; Hybrid applications; Quantum computing; Quantum humanities},
	correspondence_address = {J. Barzen; University of Stuttgart, Stuttgart, Universitätsstraße 38, 70569, Germany; email: johanna.barzen@iaas.uni-stuttgart.de},
	editor = {Ferguson D. and Pahl C. and Helfert M.},
	publisher = {Springer Science and Business Media Deutschland GmbH},
	issn = {18650929},
	isbn = {978-303072368-2},
	language = {English},
	abbrev_source_title = {Commun. Comput. Info. Sci.},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 13; Conference name: 10th International Conference on Cloud Computing and Services Science, CLOSER 2020; Conference date: 7 May 2020 through 9 May 2020; Conference code: 256809}
}

@ARTICLE{Gong2021,
	author = {Gong, Changqing and Dong, Zhaoyang and Gani, Abdullah and Qi, Han},
	title = {Quantum k-means algorithm based on trusted server in quantum cloud computing},
	year = {2021},
	journal = {Quantum Information Processing},
	volume = {20},
	number = {4},
	doi = {10.1007/s11128-021-03071-7},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85103840536&doi=10.1007%2fs11128-021-03071-7&partnerID=40&md5=f037883cb87001f082cf667c95a48c5b},
	affiliations = {School of Computer Science and Technology, Shenyang Aerospace University, Shenyang, China; Faculty of Computer Science and Information Technology, University of Malaya, Kuala Lumpur, Malaysia},
	abstract = {Quantum k-means algorithm is widely used in solving clustering problems. However, the repeated calculation of the core subroutines overloads the quantum computer with mass computing tasks. In addition, due to the decoherence of the quantum state, the computing power of personal quantum computer is limited. Therefore, it is necessary to use high-performance quantum cloud computing to complete data processing. This paper proposes a quantum k-means algorithm in quantum cloud computing, which uploads the data to the server to execute the core subroutines SwapTest (calculating the similarity between ciphertext quantum states) and GroverOptim (quantum minimization algorithm) of quantum k-means algorithm and then sends the ciphertext to the client for further decryption using the decryption key, which reduces the load of the client. An improved quantum homomorphic encryption by t-gate updating in trusted server and data processing in semi-trusted server is introduced as well. In this scheme, the trusted server assists the semi-trusted server to execute the T-gate existing in the quantum circuit, which separates the key from the ciphertext and ensure the security of the ciphertext data processing in the quantum cloud. In our experiment, the two subroutines, SwapTest and GroverOptim, are conducted in ciphertext and plaintext conditions by using IBM Qiskit to evaluate the reliability and feasibility. The result shows that the decrypted ciphertext result is the same as the plaintext. We conclude that the proposed algorithm has a better performance in reducing the cost of load in client and protecting privacy data in cloud by contrast with the original quantum k-means algorithm. © 2021, The Author(s), under exclusive licence to Springer Science+Business Media, LLC, part of Springer Nature.},
	author_keywords = {IBM Qiskit; Key update algorithm; Quantum cloud computing; Quantum computing; Quantum homomorphic encryption},
	correspondence_address = {H. Qi; School of Computer Science and Technology, Shenyang Aerospace University, Shenyang, China; email: qihan@sau.edu.cn},
	publisher = {Springer},
	issn = {15700755},
	language = {English},
	abbrev_source_title = {Quantum Inf. Process.},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 14}
}

