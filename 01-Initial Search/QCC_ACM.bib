@inproceedings{10.1145/3349266.3355614,
author = {Meldrum, Neal},
title = {The Convergence of Information and Operational Technology is Powering the Fourth Industrial Revolution: Digital Transformation is an Imperative},
year = {2019},
isbn = {9781450369213},
