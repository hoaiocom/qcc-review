articleno = {130},
numpages = {13},
keywords = {3D printing, IoT, Quantum computing, cloud computing, cryptography, fog computing, machine learning},
location = {Jaipur, India},
series = {ICIMMI '23}
}

@inproceedings{10.1145/3555962.3555964,
author = {Zhang, Meng and Fu, Yipeng and Wang, Jing and Lai, Junsen},
