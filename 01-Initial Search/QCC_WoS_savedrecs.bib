@article{ WOS:001237393900001,
Author = {Xu, Lida and Cheng, Xiangguo and Tian, Weizhong and Wang, Huanli and
   Zhang, Yan},
Title = {Cloud-Assisted Privacy-Preserving Spectral Clustering Algorithm Within a
   Multi-User Setting},
Journal = {IEEE ACCESS},
Year = {2024},
Volume = {12},
Pages = {75965-75982},
Abstract = {Spectral clustering, a powerful algorithm in the field of AI, holds a
   significant role despite its inherent high time complexity. For data
   owners grappling with limitations such as small datasets and restricted
   computational resources, harnessing the computational capabilities of
   cloud computing and aggregating data from multiple sources can yield
   precise spectral clustering results. However, explicit data uploading to
   cloud servers poses privacy risks. In response to this challenge, we
   explore the outsourcing dilemma of spectral clustering in a cloud and
   multi-user environment and propose a quantum-secure and efficient
   solution. Specifically, by employing the CKKS homomorphic encryption
   algorithm within a dual non-collusive server model, we formulate a
   comprehensive and multi-user spectral clustering outsourcing scheme. Our
   approach addresses privacy concerns by introducing secure computation
   protocols for L-2 norm, exponential function, and negative half power
   function. We elaborate on efficient computational algorithms for each
