Title = {Evaluating Efficacy of Model Stealing Attacks and Defenses on Quantum
   Neural Networks},
Booktitle = {PROCEEDING OF THE GREAT LAKES SYMPOSIUM ON VLSI 2024, GLSVLSI 2024},
Series = {Proceedings - Great Lakes Symposium on VLSI},
Year = {2024},
