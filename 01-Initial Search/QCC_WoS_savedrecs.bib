@inproceedings{ WOS:001124987200040,
Author = {Xu, Chuanqi and Erata, Ferhat and Szefer, Jakub},
Book-Group-Author = {ACM},
Title = {Exploration of Power Side-Channel Vulnerabilities in Quantum Computer
   Controllers},
Booktitle = {PROCEEDINGS OF THE 2023 ACM SIGSAC CONFERENCE ON COMPUTER AND
   COMMUNICATIONS SECURITY, CCS 2023},
Year = {2023},
Pages = {579-593},
