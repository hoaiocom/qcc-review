Title = {A Quantum Communication Based Secure Road Condition Monitoring
   Application in Intelligent Internet of Vehicular Things},
Booktitle = {2022 IEEE 19TH INDIA COUNCIL INTERNATIONAL CONFERENCE, INDICON},
