@article{ WOS:000824723900001,
Author = {Fan, Qing and He, Debiao and Chen, Jianhua and Peng, Cong and Wang,
   Lianhai},
Title = {Isoga: An Isogeny-Based Quantum-Resist Searchable Encryption Scheme
   Against Keyword Guessing Attacks},
