@inproceedings{ WOS:000978301700004,
Author = {Yavuz, Attila A. and Nouma, Saif E. and Hoang, Thang and Earl, Duncan
   and Packard, Scott},
Book-Group-Author = {IEEE},
Title = {Distributed Cyber-infrastructures and Artificial Intelligence in Hybrid
   Post-Quantum Era},
Booktitle = {2022 IEEE 4TH INTERNATIONAL CONFERENCE ON TRUST, PRIVACY AND SECURITY IN
   INTELLIGENT SYSTEMS, AND APPLICATIONS, TPS-ISA},
Year = {2022},
