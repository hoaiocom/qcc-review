@article{ WOS:000799234000036,
Author = {Xu, Gang and Cao, Yibo and Xu, Shiyuan and Liu, Xin and Chen, Xiu-Bo and
   Yu, Yiying and Wang, Xiaojun},
Title = {A Searchable Encryption Scheme Based on Lattice for Log Systems in
   Blockchain},
