@article{ WOS:001142681800714,
Author = {Qu, Zhiguo and Meng, Yunyi and Liu, Bo and Muhammad, Ghulam and Tiwari,
   Prayag},
Title = {QB-IMD: A Secure Medical Data Processing System With Privacy Protection
   Based on Quantum Blockchain for IoMT},
Journal = {IEEE INTERNET OF THINGS JOURNAL},
