@article{ WOS:001045276800002,
Author = {Shikano, Yutaka and Tamura, Kentaro and Raymond, Rudy},
Title = {Detecting Temporal Correlation via Quantum Random Number Generation},
Journal = {ELECTRONIC PROCEEDINGS IN THEORETICAL COMPUTER SCIENCE},
Year = {2020},
Number = {315},
Pages = {18-25},
Note = {9th International Conference on Quantum Simulation and Quantum Walks
   (QSQW), Marseille, FRANCE, JAN 20-24, 2020},
Abstract = {All computing devices, including quantum computers, must exhibit that
   for a given input, an output is produced in accordance with the program.
   The outputs generated by quantum computers that fulfill these
   requirements are not temporally correlated, however. In a
   quantum-computing device comprising solid-state qubits such as
   superconducting qubits, any operation to rest the qubits to their
   initial state faces a practical problem. We applied a statistical
   analysis to a collection of random numbers output from a 20-qubit
   superconducting-qubit cloud quantum computer using the simplest random
   number generation scheme. The analysis indicates temporal correlation in
   the output of some sequences obtained from the 20 qubits. This temporal
   correlation may be not related to the relaxation time of each qubit.},
