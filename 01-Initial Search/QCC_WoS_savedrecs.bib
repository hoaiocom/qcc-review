@inproceedings{ WOS:000754488100009,
Author = {Gustafson, Erik and Holzman, Burt and Kowalkowski, James and Lamm, Henry
   and Li, Andy C. Y. and Perdue, Gabriel and Isakov, V, Sergei and Martin,
   Orion and Thomson, Ross and Beall, Jackson and Ganahl, Martin and Vidal,
   Guifre and Peters, Evan and Boixo, Sergio},
Book-Group-Author = {IEEE Comp Soc},
Title = {Large scale multi-node simulations of Z<sub>2</sub> gauge theory quantum
   circuits using Google Cloud Platform},
Booktitle = {PROCEEDINGS OF SECOND INTERNATIONAL WORKSHOP ON QUANTUM COMPUTING
   SOFTWARE (QCS 2021)},
Year = {2021},
