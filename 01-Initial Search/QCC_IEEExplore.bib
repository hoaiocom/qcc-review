  month={},}@INPROCEEDINGS{10621726,
  author={Aparicio-Morales, Álvaro M. and Haghparast, Majid and Mäkitalo, Niko and Garcia-Alonso, Jose and Berrocal, Javier and Stirbu, Vlad and Mikkonen, Tommi and Murillo, Juan Manuel},
  booktitle={2024 IEEE International Conference on Software Analysis, Evolution and Reengineering - Companion (SANER-C)}, 
  title={Liquifying Quantum-Classical Software-Intensive System of Systems}, 
  year={2024},
  volume={},
  number={},
  pages={159-166},
  abstract={Software-Intensive Systems are applications that use a high amount of computational resources for the execution of complex tasks in which a constant flow of information is needed. In the current conception of the computing continuum, such computational resources are spread in the infrastructure from the Cloud to the Edge and the tasks are mapped on resources following placing and replacing strategies. In this scope, this paper puts together two emergent paradigms. On the one hand, the emergence of the Quantum Computation paradigm will allow these intensive systems to incorporate specific tasks that are unaffordable by classic computers. On the other hand, the liquid software model proposes a fluid movement of the software and information from one device to another. Therefore, in this paper, we present a broad overview of software-intensive systems from a liquid software perspective and support quantum functionalities in their processes. To provide a better understanding of how these systems can take form, we explore a case study based on weather forecasts.},
  keywords={Computers;Quantum computing;Liquids;Computational modeling;Clouds;Weather forecasting;Software;Quantum Software Engineering;Quantum computing;Liquid software;Software-Intensive Systems of Systems;Computing Continuum},
  doi={10.1109/SANER-C62648.2024.00028},
  ISSN={},
  month={March},}@INPROCEEDINGS{10393164,
  author={R, Dileep M and L, Vineeth and Avula, Sashikanth Reddy and V, Navaneeth A},
  booktitle={2023 International Conference on Evolutionary Algorithms and Soft Computing Techniques (EASCT)}, 
  title={Preserving the Privacy in IoT Medical data in Edge Computing using Modified AES Model with Discrete-Time Alternating Quantum Walk}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={The rapid growth of IoT edge devices has caused in an unprecedented explosion of data volume. Extremely often, the created data is gathered and disseminated via edge-based IoT structures. When hostile IoT devices make requests to steal private data from a cloud storage system via edge nodes, the data-sharing privacy exposure issue becomes even more concerning. We provide evolutionary privacy protection learning algorithms for an IoT data sharing scheme based on edge computing as a solution to the stated problem. Deep learning of medical models in an IoT-based healthcare scheme is described here via a federated learning approach. Using a combination of the alternating quantum walk (AQW) and the Advanced encryption standard (AES), this research suggests a new method of picture encryption. A keystream generator associated with AQW limits is used to build a likelihood distribution matrix, and the study makes use of quantum features to enhance the AES algorithm. To unlock the AES method, we must first extract certain singular values from the matrix. Elements of the probability distribution matrix stand in for Rcon in the AES algorithm. The AES algorithm's S-box and ShiftRow transformation mapping rules are then jumbled based on the increasing size of the clone probability distribution matrix. The preprocessing is done with a likelihood distribution matrix and a plaintext XOR operation, and the encryption is done with a modified version of the AES algorithm. To prevent an adversary from concluding confidential medical data via assaults like a perfect reconstruction attack, etc., this encryption technique may be applied to better safeguard local models.},
  keywords={Data privacy;Quantum computing;Federated learning;Computational modeling;Medical services;Probability distribution;Encryption;Internet of Things;Alternating quantum walk;Advanced encryption standard;Privacy Preserving;Data security},
  doi={10.1109/EASCT59475.2023.10393164},
  ISSN={},
  month={Oct},}@ARTICLE{10443474,
  author={Wang, Miao and Sun, Liwang and Cao, Zhenfu and Dong, Xiaolei},
  journal={IEEE Access}, 
  title={IPO-PEKS: Effective Inner Product Outsourcing Public Key Searchable Encryption From Lattice in the IoT}, 
  year={2024},
  volume={12},
  number={},
  pages={28926-28937},
  abstract={Lightweight devices in the Internet of Things (IoT) typically need to store massive data on a cloud server with strong processing and storage capabilities for later retrieval and usage. Since these data contain the participant’s sensitive information, they cannot be delivered directly to the cloud server. Public-key Encryption with Keyword Search (PEKS) allows customers to search for target encrypted files using keywords. However, the majority of PEKS implementations are unable to repel malicious quantum-capable attackers. And with regard to forward security, they must search for many rounds to obtain the necessary data. To resolve these concerns, we propose a comprehensive Inner Product Outsourcing PEKS system (IPO-PEKS) with forward security based on LWE assumptions, which raises search efficiency by allowing authorized clients to find the information they desire in a single round and achieves more fine-grained searches. Furthermore, we offer an inner product outsourcing calculation technique that allows the server to compute the inner product result without knowing the details of both parties in order to conceal the relevant privacy data of transmitting and decryption states. The paradigm can be utilized for efficient state transition through the use of parallel computing to accomplish the target of one round of iteration.},
  keywords={Public key;Servers;Encryption;Keyword search;Security;Cloud computing;Internet of Things;Outsourcing;Cloud computing;Storage management;PEKS;forward security;inner product outsourcing;LWE assumptions;quantum-resist},
  doi={10.1109/ACCESS.2024.3368908},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{10621559,
  author={Mahanipour, Afsaneh and Khamfroush, Hana},
  booktitle={2024 20th International Conference on Distributed Computing in Smart Systems and the Internet of Things (DCOSS-IoT)}, 
  title={Enhancing IoT Security: A Novel Feature Engineering Approach for ML-Based Intrusion Detection Systems}, 
  year={2024},
  volume={},
  number={},
  pages={548-555},
  abstract={The integration of Internet of Things (IoT) applications in our daily lives has led to a surge in data traffic, posing significant security challenges. IoT applications using cloud and edge computing are at higher risk of cyberattacks because of the expanded attack surface from distributed edge and cloud services, the vulnerability of IoT devices, and challenges in managing security across interconnected systems leading to oversights. This led to the rise of ML-based solutions for intrusion detection systems (IDSs), which have proven effective in enhancing network security and defending against diverse threats. However, ML-based IDS in IoT systems encounters challenges, particularly from noisy, redundant, and irrelevant features in varied IoT datasets, potentially impacting its performance. Therefore, reducing such features becomes crucial to enhance system performance and minimize computational costs. This paper focuses on improving the effectiveness of ML-based IDS at the edge level by introducing a novel method to find a balanced trade-off between cost and accuracy through the creation of informative features in a two-tier edge-user IoT environment. A hybrid Binary Quantum-inspired Artificial Bee Colony and Genetic Programming algorithm is utilized for this purpose. Three IoT intrusion detection datasets, namely NSL-KDD, UNSW-NB15, and BoT-IoT, are used for the evaluation of the proposed approach. Performance analysis is conducted using various evaluation metrics such as accuracy, sensitivity, specificity, and False Positive Rate (FPR) are employed, while the cost of the IDS system is assessed based on computational time. The results are compared with existing methods in the literature, revealing that the IDS performance can be enhanced with fewer features, consequently reducing computational time, through the proposed method. This offers a better performance-cost trade-off for the IDS system.},
  keywords={Cloud computing;Costs;Accuracy;Sensitivity;Quantum computing;Image edge detection;System performance;Binary Quantum-inspired Artificial Bee Colony Algorithm;Feature Construction;Feature Selection;Genetic Programming;Intrusion Detection Systems},
  doi={10.1109/DCOSS-IoT61029.2024.00086},
  ISSN={2325-2944},
  month={April},}@INPROCEEDINGS{10270401,
  author={Kollannur, Aruna Shaju and Manoj, B. S.},
  booktitle={2023 IEEE 4th Annual Flagship India Council International Subsections Conference (INDISCON)}, 
  title={A Novel Homomorphic Computing Approach of Edge Internet of Things}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={Edge Internet of Things (EIoT) involves computing at edge servers rather than at the cloud, thus primarily reducing latency and bandwidth issues. A traditional EIoT system often ignores the security issues that prevail due to power and computational limitations. Additionally, a hierarchical EIoT system essentially acts like a cloud system requiring security access to be given to a third party for data analysis. The concept of homomorphic encryption can be introduced in this scenario where computation can be done on encrypted data thus removing the need to give third-party access to sensitive data. A novel homomorphic encryption is proposed by enhancing the EIGamal algorithm with the capability of learning with errors to adapt it as a post-quantum cryptographic algorithm. This enhanced algorithm cannot be decrypted purely using the knowledge needed to decrypt the original algorithm, with the same key pair.},
  keywords={Cloud computing;Additives;Limiting;Data analysis;Public key;Bandwidth;Internet of Things;IoT;Edge IoT;Encryption;Edge Security;EIGamal;Learning with Errors;Post-Quantum Cryptography},
  doi={10.1109/INDISCON58499.2023.10270401},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{10366718,
  author={Panda, Sanjaya Kumar and Sen, Shidhanta},
