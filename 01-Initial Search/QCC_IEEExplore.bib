  number={},
  pages={633-636},
  abstract={Educational informatization is a major trend in the development of education in the world today, and the construction of digital campuses is a concrete manifestation of the degree of educational informatization. Quantum computers perform calculations through the microscopic properties of quantum mechanics. In recent years, breakthroughs in experimental technology have led some technology companies to launch quantum computers for the public to gain a deeper understanding of quantum algorithms and solve some practical problems. Based on the theory of quantum cloud computing, this paper studies the intelligent retrieval platform of digital resources in colleges and universities.},
  keywords={Computers;Cloud computing;Quantum computing;Quantum algorithm;Microscopy;Education;Quantum mechanics;University Digital Resources;Intelligent Search;Quantum Cloud Computing;Numerical Analysis},
  doi={10.1109/I-SMAC52330.2021.9640989},
  ISSN={2768-0673},
  month={Nov},}@INPROCEEDINGS{9825324,
  author={Belmahdi, Raouf and Mechta, Djamila and Harous, Saad and Bentaleb, Abdelhak},
  booktitle={2022 International Wireless Communications and Mobile Computing (IWCMC)}, 
  title={SQGA: Quantum Genetic Algorithm-based Workflow Scheduling in Fog-Cloud Computing}, 
  year={2022},
  volume={},
  number={},
  pages={131-136},
  abstract={Fog computing represents an extension of the Cloud infrastructure, which allows the improvement of the performance of IoT applications. The problem of task scheduling represents a challenge in this type of environment, with the aim of how to allocate the tasks to the different nodes of the Fog-Cloud infrastructure, in order to minimize makespan, cost, response time, and energy. In this paper, we propose SQGA— an algorithm to improve the workflow scheduling in Fog-Cloud environment. This algorithm is based on the quantum genetic algorithm QGA and aims to improve the makespan of applications deployed in the Fog-Cloud computing environment. The proposed SQGA scheduling algorithm is compared to the classical genetic algorithm and the First Come First Served algorithm. The experiment results show that the proposed SQGA algorithm is more efficient in makespan, and adapts better to the available resources.},
  keywords={Wireless communication;Quantum computing;Scheduling algorithms;Simulation;Quality of service;Scheduling;Time factors;Fog Computing;Cloud Computing;IoT;Tasks scheduling;Quantum genetic algorithm;Makespan},
  doi={10.1109/IWCMC55113.2022.9825324},
  ISSN={2376-6506},
  month={May},}@INPROCEEDINGS{10435310,
  author={Niranjan, Priyanshu and Thenmozhi, M.},
  booktitle={2023 Second International Conference on Advances in Computational Intelligence and Communication (ICACIC)}, 
  title={Priority-Induced Round-robin scheduling algorithm with dynamic time quantum for Cloud computing environment}, 
  year={2023},
  volume={},
  number={},
  pages={1-5},
  abstract={Cloud computing provides a virtual platform that allows resources to be accessed through the Internet. As cloud environment needs to serve multiple requests, efficient task scheduling is required to maintain its performance. Round Robin Scheduling algorithm is one of the most employed algorithms for task scheduling in cloud environment. In this paper, an improved Round Robin algorithm has been proposed. The proposed algorithm introduces dynamic time quantum and priority for scheduling the tasks. It is carried out by categorizing the tasks based on their urgency to complete the task from the ready queue. A comparative study is also established and inferred that the proposed algorithm is comparatively enhanced and is optimum for the Cloud Computing environment.},
  keywords={Cloud computing;Quantum computing;Heuristic algorithms;Dynamic scheduling;Resource management;Task analysis;Round robin;Round Robin;Cloud Computing;Dynamic Time Quantum;Priority Queue;Ready Queue},
  doi={10.1109/ICACIC59454.2023.10435310},
  ISSN={},
  month={Dec},}@ARTICLE{10356051,
  author={Marosi, Attila Csaba and Farkas, Attila and Máray, Tamás and Lovas, Róbert},
  journal={IEEE Access}, 
  title={Toward a Quantum-Science Gateway: A Hybrid Reference Architecture Facilitating Quantum Computing Capabilities for Cloud Utilization}, 
  year={2023},
  volume={11},
  number={},
  pages={143913-143924},
  abstract={The emerging availability of quantum compute resources fosters the examination of its exploitation possibilities in different scientific domains, like artificial intelligence, manufacturing or finance. A significant number of research scientists primarily rely on cloud computing infrastructures while conducting research, whereas access to real (mostly remote) quantum hardware resources requires the deployment and proper configuration of different software components. In this paper we present a hybrid, cloud-based reference architecture that lowers the entry barrier to start new experiments with a wide range of quantum compute resources. The solution facilitates the execution of distributed quantum computing simulations in traditional cloud environments, and also access to several remote quantum compute resources. The reference architecture is highly portable to various cloud platforms, resulting in efficient adaptation and application possibilities by research communities. The paper describes our related experiences using commercial cloud providers and on the federated, OpenStack-based research infrastructure of the Hungarian Research Network (abbreviated as HUN-REN).},
  keywords={Quantum computing;Cloud computing;Computer architecture;Hardware;Computational modeling;Logic gates;Qubit;Cloud computing;quantum computing;reference architecture;simulation;science gateway},
  doi={10.1109/ACCESS.2023.3342749},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{10133995,
  author={Surendhar, K and Chamarthy, Lakshmi Sravanthi and Priyadharshini, DM and Keerthana, Gandlaparthi and Sinha, Garima and Kumar, A. Vijay},
  booktitle={2023 International Conference on Inventive Computation Technologies (ICICT)}, 
  title={Enhancement of Round Robin Algorithm with Dynamic Quantum in Cloud Computing}, 
  year={2023},
  volume={},
  number={},
  pages={799-803},
  abstract={Load Balancing is the process of distributing the computing load equally among the available resources which are pooled. With the rapidly advancing information technology industry, load balancing becomes a necessity. The load balancer equally distributes the load between the available resources, the lack of load balancing makes the single resources to bare huge amounts of requests. It also makes the resources highly available for the users. Load Balancing is used in both on-premises and off-premises environments. The load balancing mainly depends on a certain algorithm for the operation. There are many load-balancing algorithms used. The most widely used algorithm is the load balancing algorithm is Round Robin. Each task in the Round-Robin Algorithm is carried out based on the time quantum. Although widely used, the Round Robin algorithm has some issues that are trying to solve using this research study. Some of the major issues focused here are the lack of dynamic time quantum allocation and more context switching and not prioritizing tasks. This research study initially reviews the cloud computing and its deployment strategies. Then, this research study presents a complete overview on the importance of load balancing, the most widely used load balancing algorithm with cloud environment, and its shortcomings. Finally, a research idea has been proposed for the enhancement of Round Robin load balancing algorithm.},
  keywords={Cloud computing;Quantum computing;Heuristic algorithms;Switches;Load management;Servers;Resource management;Cloud computing;Round-Robin algorithm;Load balancing;Priority;Dynamic Quantum},
  doi={10.1109/ICICT57646.2023.10133995},
  ISSN={2767-7788},
  month={April},}@INPROCEEDINGS{9788373,
  author={Lu, Xikui and Tang, Shihua},
  booktitle={2022 6th International Conference on Intelligent Computing and Control Systems (ICICCS)}, 
  title={Synchronous Dislocation Scheduling Quantum Algorithm Optimization in Virtual Private Cloud Computing Environment}, 
  year={2022},
  volume={},
  number={},
  pages={596-599},
  abstract={This article studies the optimization of the synchronization and misalignment scheduling quantum algorithm in the enterprise private cloud computing environment. It is necessary to effectively reduce the operating cost of the data center while meeting user satisfaction. For this reason, this article draws on the queuing theory and stochastic model in operations research., To reduce the number of active physical machines while achieving user satisfaction as the goal. By introducing user expenditure indicators, the hybrid group intelligent optimization algorithm can greatly reduce user expenditures while taking into account higher efficiency. A large number of experiments have been conducted on the CloudSim cloud computing simulation experiment platform for the task scheduling and resource scheduling algorithms proposed above.},
  keywords={Cloud computing;Quantum algorithm;Processor scheduling;Scheduling algorithms;Computational modeling;Stochastic processes;Scheduling;Synchronous Dislocation;Scheduling Quantum;Virtual Private;Cloud Computing},
  doi={10.1109/ICICCS53718.2022.9788373},
  ISSN={2768-5330},
  month={May},}@INPROCEEDINGS{9972028,
  author={Shah, Rajvir},
  booktitle={2022 IEEE 3rd Global Conference for Advancement in Technology (GCAT)}, 
  title={The Conventional Security of Cloud Computing and the Growing Threat to Quantum Computing}, 
  year={2022},
  volume={},
  number={},
  pages={1-7},
  abstract={Cloud computing has turned into an important technology of our time. It has drawn attention due to its, availability, dynamicity, elasticity and pay as per use pricing mechanism this made multiple organizations to shift onto the cloud platform. It leverages the cloud to reduce administrative and backup overhead. Cloud computing offers a lot of versatility. Quantum technology, on the other hand, advances at a breakneck pace. Experts anticipate a positive outcome and predict that within the next decade, powerful quantum computers will be available. This has and will have a substantial impact on various sciences streams such as cryptography, medical research, and much more. Sourcing applications for business and informational data to the cloud, presents privacy and security concerns, which have become crucial in cloud installation and services adoption. To address the current security weaknesses, researchers and impacted organizations have offered several security techniques in the literature. The literature also gives a thorough examination of cloud computing security and privacy concerns.},
  keywords={Computers;Cloud computing;Quantum computing;Qubit;Organizations;Probability distribution;Manufacturing;quantum computing;security issue in cloud computing;error correction;qubit manufacturing issues},
  doi={10.1109/GCAT55367.2022.9972028},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10625286,
  author={Chhabra, Manish and E, Rajesh},
  booktitle={2024 2nd International Conference on Sustainable Computing and Smart Systems (ICSCSS)}, 
  title={Multi-Cloud Environments With Quantum Cloud Computing for Secure Data Transfer using Two-Tier Homomorphic Encryption}, 
  year={2024},
  volume={},
  number={},
  pages={663-670},
  abstract={Nowadays, data transfer has been vast online and on social media. Hence data security in the various cloud environments has been enabled to send and receive data. While transferring the data, some issues in the cloud environment are the lack of connectivity, control, and the insecurity of the password and the data. To handle this problem, data security and the internet connection have to be enabled for the cloud environments. The method of homomorphic encryption and the transport layer security protocol have been enabled for the analysis. The homomorphic encryption method converts the data to the ciphertext for making the text in the original format. Then the transport layer security protocol is used to authenticate the data, and the data integrity of the communication can be executed. The proposed system performs better than current protocols like Dynamic Level Based Integrity Checking Protocol, TLS protocol, and Lightweight Cryptosystem, making it a potential option for jobs involving safe and effective cloud information processing. Overall, the proposed Two-tier Homomorphic Encryption system exhibits improved execution time, security strength, resource utilization, communication overhead, and user satisfaction. It addresses issues such as connectivity gaps, control deficiencies, and password and data insecurity, making it an effective method for boosting cloud security in quantum cloud computing environments.},
  keywords={Protocols;Quantum computing;Cloud computing security;Distributed databases;Passwords;Information processing;Data transfer;Cipher Text;Data Management;Data Security;Homomorphic Encryption;Security Protocol},
  doi={10.1109/ICSCSS60660.2024.10625286},
  ISSN={},
  month={July},}@ARTICLE{10261941,
  author={Irshad, Reyazur Rashid and Hussain, Shahid and Hussain, Ihtisham and Nasir, Jamal Abdul and Zeb, Asim and Alalayah, Khaled M. and Alattab, Ahmed Abdu and Yousif, Adil and Alwayle, Ibrahim M.},
  journal={IEEE Access}, 
  title={IoT-Enabled Secure and Scalable Cloud Architecture for Multi-User Systems: A Hybrid Post-Quantum Cryptographic and Blockchain-Based Approach Toward a Trustworthy Cloud Computing}, 
  year={2023},
  volume={11},
  number={},
  pages={105479-105498},
  abstract={Cloud computing has revolutionized organizational operations by providing convenient, on-demand access to resources. The emergence of the Internet of Things (IoT) has introduced a new paradigm for collaborative computing, leveraging sensors and devices that generate and process vast amounts of data, thereby resulting in challenges related to scalability and security, making the significance of conventional security methods even more pronounced. Consequently, in this paper, we propose a novel Scalable and Secure Cloud Architecture (SSCA) that integrates IoT and cryptographic techniques, aiming to develop scalable and trustworthy cloud systems, thus enabling multi-user systems and facilitating simultaneous access to cloud resources by multiple users. The design adopts a decentralized approach, utilizing multiple cloud nodes to handle user requests efficiently and incorporates Multicast and Broadcast Rekeying Algorithm (MBRA) to ensure the privacy and confidentiality of user information, utilizing a hybrid cryptosystem that combines MBRA, Post Quantum Cryptography (PQC) and blockchain technology. Leveraging IoT devices, the architecture gathers data from distributed sensing resources and ensures the security of collected information through robust MBRA-PQC encryption algorithms, while the blockchain ensures that the confidential data is stored in distributed and immutable records. The proposed approach is applied to several datasets and the effectiveness is validated through various performance metrics, including response time, throughput, scalability, security, and reliability. The results highlight the effectiveness of the proposed SSCA, showcasing a notable reduction in response time by 1.67 seconds and 0.97 seconds for 250 and 1000 devices, respectively, in comparison to the MHE-IS-CPMT. Likewise, SSCA demonstrated significant improvements in the AUC values, exhibiting enhancements of 6.30%, 6.90%, 7.60%, and 7.30% at the 25-user level, and impressive gains of 5.20%, 9.30%, 11.50%, and 15.40% at the 50-user level when compared to the MHE-IS-CPMT, EAM, SCSS, and SHCEF models, respectively.},
  keywords={Cloud computing;Security;Internet of Things;Computer architecture;Organizations;Blockchains;Encryption;Artificial intelligence;Decentralized applications;Blockchain;IoT-enabled cloud architecture;post-quantum cryptography;artificial intelligence;scalability;multi-user systems;decentralized approach},
  doi={10.1109/ACCESS.2023.3318755},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{9594228,
  author={Iftikhar, Zainab and Iftikhar, Malayka and Shah, Munam Ali},
  booktitle={2021 26th International Conference on Automation and Computing (ICAC)}, 
  title={Quantum Safe Cloud Computing Using Hash-based Digital Signatures}, 
  year={2021},
  volume={},
  number={},
  pages={1-6},
  abstract={Cloud computing provides its clients with on-demand resources, such as storage and performance. The idea behind cloud computing is sharing of resources. With growth of cloud computing, the risks of security and privacy are arising. Also, the cloud resources are not secure against quantum computers, as quantum computing threatens the best security controls implemented today, that include ECC (Elliptic Curve Cryptography), RSA, DSA (Digital Signature Algorithm) and ECDSA (Elliptic Curve Digital Signature Algorithm). Fortunately, there are quantum-safe digital signatures called Hash-based Signatures, which can also protect integrity, authentication and privacy on the cloud. In this work, a hash-based digital signature scheme called WOTS is implemented to avoid security risks imposed by quantum computing on cloud computing. The implementation is carried out in CloudSim simulator. Apart from achieving quantum safety, the results show that our proposed solution is computationally cost effective.},
  keywords={Computers;Cloud computing;Privacy;Quantum computing;Costs;Elliptic curve cryptography;Safety;Cloud computing;quantum cryptography;HBSS (hash-based signature scheme);OTS (one-time signatures);WOTS (Winternitz OTS);CloudSim},
  doi={10.23919/ICAC50006.2021.9594228},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{9668289,
  author={Ravi, Gokul Subramanian and Smith, Kaitlin N. and Gokhale, Pranav and Chong, Frederic T.},
  booktitle={2021 IEEE International Symposium on Workload Characterization (IISWC)}, 
  title={Quantum Computing in the Cloud: Analyzing job and machine characteristics}, 
  year={2021},
  volume={},
  number={},
  pages={39-50},
  abstract={As the popularity of quantum computing continues to grow, quantum machine access over the cloud is critical to both academic and industry researchers across the globe. And as cloud quantum computing demands increase exponentially, the analysis of resource consumption and execution characteristics are key to efficient management of jobs and resources at both the vendor-end as well as the client-end. While the analysis of resource consumption and management are popular in the classical HPC domain, it is severely lacking for more nascent technology like quantum computing. This paper is a first-of-its-kind academic study, analyzing various trends in job execution and resources consumption / utilization on quantum cloud systems. We focus on IBM Quantum systems and analyze characteristics over a two year period, encompassing over 6000 jobs which contain over 600,000 quantum circuit executions and correspond to almost 10 billion “shots” or trials over 20+ quantum machines. Specifically, we analyze trends focused on, but not limited to, execution times on quantum machines, queuing/waiting times in the cloud, circuit compilation times, machine utilization, as well as the impact of job and machine characteristics on all of these trends. Our analysis identifies several similarities and differences with classical HPC cloud systems. Based on our insights, we make recommendations and contributions to improve the management of resources and jobs on future quantum cloud systems.},
  keywords={Industries;Computers;Quantum system;Quantum computing;Market research;Throughput;Quantum circuit;quantum computing;queuing;job execution;resource consumption;cloud systems},
  doi={10.1109/IISWC53511.2021.00015},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10008330,
  author={Al-Shammare, Haifa and Al-Otaiby, Nehal},
  booktitle={2022 14th International Conference on Computational Intelligence and Communication Networks (CICN)}, 
  title={An Implementation of a New Proposed Round-Robin Algorithm with Smart Time Quantum in Cloud Computing Environment}, 
  year={2022},
  volume={},
  number={},
  pages={289-296},
  abstract={The popularity of cloud computing platforms has risen dramatically in recent years. As cloud computing serves millions of users at the same time, it must be able to handle all those users' demands efficiently. Thus, choosing a suitable scheduling algorithm is crucial in the cloud computing environment in order to ensure efficient performance with a reasonable degree of quality of service (QoS). The primary goal of this research is to empirically implement and evaluate a recently proposed Round-Robin algorithm with smart time quantum (RR-STQ) in a cloud computing environment, as well as, to enhance the RR-STQ with a dynamic smart time quantum. The CloudSim tool was used to simulate the cloud computing platform to implement RR-STQ and evaluate it with several algorithms using different scenarios. In addition, three scheduling performance metrics were used in the evaluation process. In all comparison scenarios, the (RR-STQ) achieved a significant improvement rate in terms of average response time (RT). Moreover, (RR-STQ) has a better performance in the average turnaround time (TAT), waiting time (WT), and response time (RT) than the traditional RR algorithm. Also, the implemented algorithm (RR-STQ) with dynamic time quantum has a better performance than static time quantum. Based on the evaluation results, it is beneficial to integrate the RR algorithm with other scheduling models such as shortest job first (SJF) to enhance the WT and TAT. Furthermore, the investigations revealed that the dynamic time quantum improves the performance of the RR algorithm.},
  keywords={Measurement;Cloud computing;Quantum computing;Scheduling algorithms;Heuristic algorithms;Computational modeling;Quality of service;Scheduling Algorithm;Round Robin;Dynamic Time Quantum;Arrival Time;Burst Time;Waiting Time;Turn Around Time;Response Time;Cloud computing;CloudSim},
  doi={10.1109/CICN56167.2022.10008330},
  ISSN={2472-7555},
  month={Dec},}@INPROCEEDINGS{10643952,
  author={Nguyen, Hoa T. and Usman, Muhammad and Buyya, Rajkumar},
  booktitle={2024 IEEE 17th International Conference on Cloud Computing (CLOUD)}, 
  title={DRLQ: A Deep Reinforcement Learning-based Task Placement for Quantum Cloud Computing}, 
  year={2024},
  volume={},
  number={},
  pages={475-481},
  abstract={The quantum cloud computing paradigm presents unique challenges in task placement due to the dynamic and heterogeneous nature of quantum computation resources. Traditional heuristic approaches fall short in adapting to the rapidly evolving landscape of quantum computing. This paper proposes DRLQ, a novel Deep Reinforcement Learning (DRL)- based technique for task placement in quantum cloud computing environments, addressing the optimization of task completion time and quantum task scheduling efficiency. It leverages the Deep Q Network (DQN) architecture, enhanced with the Rainbow DQN approach, to create a dynamic task placement strategy. This approach is one of the first in the field of quantum cloud resource management, enabling adaptive learning and decision-making for quantum cloud environments and effectively optimizing task placement based on changing conditions and resource availability. We conduct extensive experiments using the QSimPy simulation toolkit to evaluate the performance of our method, demonstrating substantial improvements in task execution efficiency and a reduction in the need to reschedule quantum tasks. Our results show that utilizing the DRLQ approach for task placement can significantly reduce total quantum task completion time by 37.81 % to 72.93% and prevent task rescheduling attempts compared to other heuristic approaches.},
  keywords={Cloud computing;Quantum computing;Processor scheduling;Decision making;Computer architecture;Dynamic scheduling;Deep reinforcement learning;quantum cloud;task placement;resource man-agement;reinforcement learning;quantum cloud scheduling},
  doi={10.1109/CLOUD62652.2024.00060},
  ISSN={2159-6190},
  month={July},}@ARTICLE{8409367,
  author={Keinan, Shahar and Hatcher Frush, Elizabeth and Shipman, William J.},
  journal={Computing in Science & Engineering}, 
  title={Leveraging Cloud Computing for In-Silico Drug Design Using the Quantum Molecular Design (QMD) Framework}, 
  year={2018},
  volume={20},
  number={4},
  pages={66-73},
  abstract={The authors present quantum molecular design, a novel cost-saving automated framework for de novo computational drug design. This technology not only addresses many of the challenges faced in the computer-aided drug design field by using highly accurate physics-based models, it also dramatically lowers costs by leveraging an AI heuristic search algorithm with targeted chemical space.},
  keywords={Cloud computing;Distributed computing;Drugs;Proteins;Artificial intelligence;Heuristic algorithms;Scientific computing;Biomedical engineering;Supercomputers;distributed applications;discrete mathematics;medicine and science;chemistry;supercomputing;scientific computing;Supercomputing-Enabled Advances in Science and Engineering},
  doi={10.1109/MCSE.2018.042781327},
  ISSN={1558-366X},
  month={Jul},}@INPROCEEDINGS{10502533,
  author={Goswami, Siddharth and Sharma, Sachin},
  booktitle={2024 IEEE International Conference on Interdisciplinary Approaches in Technology and Management for Social Innovation (IATMSI)}, 
  title={Artificial Intelligence, Quantum Computing and Cloud Computing Enabled Personalized Medicine in Next Generation Sequencing Bioinformatics}, 
  year={2024},
  volume={2},
